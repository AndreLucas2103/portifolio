{
    "extends": ["next", "next/core-web-vitals", "prettier", "plugin:@typescript-eslint/recommended"],
    "rules": {
        "@next/next/no-img-element": "off",
        "@typescript-eslint/no-unused-vars": [
            "warn", // or "error"
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_",
                "caughtErrorsIgnorePattern": "^_"
            }
        ]
    }
}
